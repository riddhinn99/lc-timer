{
    "manifest_version": 2,
    "name": "Leetcode Timer",
    "version": "1.0",
    "description": "Timer for Leetcode questions.",
    
    "icons": {
        "128": "assets/images/icon128.png"
    },

    "browser_action": {
        "default_icon": {
            "128": "assets/images/icon128.png"
        },
        "default_title": "Leetcode Timer",
        "default_popup": "popup.html"
    },

    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content.js"]
    }]
    
}